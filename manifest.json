{
  "name" :"myGesAverage",
  "version" :"1.0",
  "manifest_version": 2,
  "description" :"Adds to the my ges website the ability to calculate and compare its mean.",
  "author": "Vincent Talloir",
  "browser_action" :
  {
      "name": "myGesAverage"
  },
  "icons" : {
    "64":"img/icone.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [{
    "matches": [
      "http://www.myges.fr/student/marks*"
    ],
    "js": [
      "common/jquery-3.1.1.min.js",
      "js/notif.js"
    ]
  }]
}